{
    "name": "BuzzFlyHC",
    "version": "1.0.0",
    "description": "General Template Starting Point For NG FFWD Exercises",
    "displayName": "Buzz Fly",
    "main": "main.js",
    "engines": {
        "node": "^10.16.0"
    },
    "types": "./cocos2d/cocos2d-lib.d",
    "scripts": {
        "start": "run-script-os",
        "start:linux:darwin": "node node_modules/@scilearn/ngffwd-node-processes/node/run.js",
        "start:win32": "node node_modules/@scilearn/ngffwd-node-processes/node/runPrevious.js",
        "test": "jest",
        "debug:jest": "node --debug-brk ./node_modules/.bin/jest --runInBand",
        "build": "run-script-os",
        "build:linux:darwin": "cross-env NODE_ENV=production node node_modules/@scilearn/ngffwd-node-processes/node/buildProdDarwin.js",
        "build:win32": "cross-env NODE_ENV=production node node_modules/@scilearn/ngffwd-node-processes/node/buildProdWindow.js",
        "build:watch": "run-script-os",
        "build:watch:darwin:linux": "cross-env NODE_ENV=development node node_modules/@scilearn/ngffwd-node-processes/node/buildWatchDarwin.js",
        "build:watch:win32": "cross-env NODE_ENV=development node node_modules/@scilearn/ngffwd-node-processes/node/buildWatchWindow.js",
        "copyRes": "node node_modules/@scilearn/ngffwd-node-processes/node/copyRes.js",
        "build-native": "node node_modules/@scilearn/ngffwd-node-processes/node/build.native.js",
        "build:save": "node node_modules/@scilearn/ngffwd-node-processes/node/saveBuild.js",
        "optimize": "node node_modules/@scilearn/ngffwd-node-processes/node/optimizejs.js",
        "initSubmodule": "node node_modules/@scilearn/ngffwd-node-processes/node/initializeSubmodule.js",
        "postinstall": "npm run build",
        "node-ids-generator": "node ./node_modules/@scilearn/ngffwd-node-processes/node/BUZHC/nodeIdsCollector.js"
    },
    "author": "temp <temp@ppl.com>",
    "license": "MIT",
    "dependencies": {
        "@scilearn/learnflow-sdk": "latest",
        "@scilearn/ngffwd-node-processes": "latest",
        "@types/bluebird": "^3.5.4",
        "@types/es6-shim": "^0.31.33",
        "@types/lodash": "^4.14.64",
        "alea": "0.0.9",
        "archiver": "^1.3.0",
        "bluebird": "^3.5.0",
        "body-parser": "^1.17.2",
        "bootstrap": "^3.3.7",
        "browserify": "^16.2.2",
        "case": "^1.5.2",
        "colors": "^1.1.2",
        "compression": "^1.6.2",
        "connect": "^3.6.2",
        "cross-env": "^5.0.1",
        "es6-shim": "^0.35.3",
        "expose-loader": "^0.7.3",
        "express": "^4.15.3",
        "fs-extra": "^3.0.1",
        "gm": "^1.23.0",
        "gulp": "^4.0.2",
        "imagemin": "^5.3.1",
        "imagemin-jpegtran": "^6.0.0",
        "imagemin-pngquant": "^8.0.0",
        "jquery": "^3.3.1",
        "js-yaml": "^3.10.0",
        "json-loader": "^0.5.4",
        "json2yaml": "^1.1.0",
        "lodash": "^4.17.4",
        "meow": "^3.7.0",
        "moment": "^2.18.1",
        "node-fetch": "^1.6.3",
        "open": "^7.0.0",
        "parallel-webpack": "^1.6.1",
        "path": "^0.12.7",
        "pixi.js-legacy": "^5.2.0",
        "planout": "^4.0.0",
        "plist": "^2.1.0",
        "query-string": "^5.0.0",
        "run-script-os": "^1.0.3",
        "rxjs": "^5.4.0",
        "serve-static": "^1.12.3",
        "sessionstorage": "^0.1.0",
        "shelljs": "^0.7.8",
        "spritesmith": "^3.1.1",
        "ts-loader": "^2.1.0",
        "tsify": "^4.0.0",
        "typedoc": "^0.16.9",
        "typescript": "^2.3.2",
        "util": "^0.10.3",
        "uuid": "^3.1.0",
        "watchify": "^3.11.0",
        "webfontloader": "^1.6.28",
        "webpack": "^2.5.1",
        "webpack-dev-server": "^2.4.5",
        "webpack-rxjs-externals": "^1.0.0",
        "webpack-visualizer-plugin": "^0.1.11",
        "yaml-loader": "^0.4.0"
    },
    "devDependencies": {
        "@types/chai": "^3.5.2",
        "@types/jest": "^20.0.2",
        "cocos-fetch": "^1.0.0",
        "exports-loader": "^0.6.4",
        "imports-loader": "^0.7.1",
        "jasmine-core": "^2.6.2",
        "jest": "^24.8.0",
        "karma": "^4.4.1",
        "karma-chrome-launcher": "^2.1.1",
        "karma-jasmine": "^1.1.0",
        "ts-jest": "^24.0.2"
    },
    "jest": {
        "moduleFileExtensions": [
            "ts",
            "tsx",
            "js"
        ],
        "transform": {
            "^.+\\.(ts|tsx)$": "ts-jest"
        },
        "setupFiles": [
            "./node_modules/@scilearn/learnflow-sdk/lib/globals.js"
        ],
        "globals": {
            "__TEST__": true
        },
        "testRegex": "/__tests__/.*\\.(ts|tsx|js)$"
    }
}
